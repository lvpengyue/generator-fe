  "dependencies": {
    <%# es6 support %>
    "babel-runtime": "^6.22.0",
    <%# main frameworks %>
    "vue": "^2.3.3",
    "jquery": "^3.0.0",
    "vue-router": "^2.5.3",
    "vuex": "^2.3.1",
    "vuex-persistedstate": "^1.4.1",
    "normalizr": "^3.1.0",
    "localforage": "^1.5.0",
